{
  "name": "tictactoe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.1.1",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "replace-array-index": "^0.0.1",
    "sweetalert": "^2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.js",
    "lint:fix": "eslint --fix ./src/**/*.js",
    "test:watch": "jest --watchAll",
    "test:update": "jest --updateSnapshot",
    "prepush": "npm run test",
    "postinstall": "rm -rf .git/hooks/pre-push && node node_modules/husky/bin/install.js && rm -rf .git/hooks/pre-commit"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-react-16-adapter-setup": "^0.1.0",
    "husky": "^0.13.4",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "react-test-renderer": "^16.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(css)$": "<rootDir>/__mocks__/custom-file-loader.js"
    },
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "enzyme-react-16-adapter-setup"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
